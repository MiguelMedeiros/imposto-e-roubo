{
  "name": "imposto-e-roubo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-brands-svg-icons": "^5.8.1",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "history": "^4.9.0",
    "i": "^0.3.6",
    "node-sass": "^4.11.0",
    "npm": "^6.9.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-facebook-pixel": "^0.1.2",
    "react-ga": "^2.5.7",
    "react-headroom": "^2.2.8",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-share": "^2.4.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./src/styles --include-path ./node_modules src/ -o ./src/css",
    "watch-css": "node-sass --include-path ./src/styles --include-path ./node_modules src/ -o ./src/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "sass": "sass --watch src/styles/scss:src/styles/css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
